<h1>Welcome to Twittest</h1>
<%= form_tag session_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username, nil %>

  <%= label_tag :password %>
  <%= password_field_tag :password, nil %>


  <%= submit_tag "Log in", class: 'btn btn-sm btn-info'%>
<% end %>

<hr>

<%= link_to "Sign Up!", new_user_path, class: 'btn btn-sm btn-success' %>
